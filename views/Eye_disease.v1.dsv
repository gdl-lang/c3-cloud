<#assign exudate_or_retinal_thickening_within_one_disc_diameter_or_circinate_or_group_exudates_within_the_macula__cds_data = eye_disease_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0015}']">
<#assign other_pathological_findings__cds_data = eye_disease_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0018}']">
<#assign event_time_cds_data = eye_disease_entry_cds_data + "[0].elements['/data/events/time']">
<#assign sudden_loss_of_vision_or_rubeosis_iridis_or_pre_retinal_or_vitreous_hemorrhage_or_retinal_detachment__cds_data = eye_disease_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0010}']">
<#assign recommendation_ehr_data = eye_disease_entry_ehr_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0022}']">
