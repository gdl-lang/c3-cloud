<#assign new_vessel_formation__def = eye_disease_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0012}']">
<#assign exudate_or_retinal_thickening_within_one_disc_diameter_or_circinate_or_group_exudates_within_the_macula__def = eye_disease_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0015}']">
<#assign other_pathological_findings__def = eye_disease_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0018}']">
<#assign event_time_def = eye_disease_entry_def + ".elements['/data/events/time']">
<#assign sudden_loss_of_vision_or_rubeosis_iridis_or_pre_retinal_or_vitreous_hemorrhage_or_retinal_detachment__def = eye_disease_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0010}']">
